<?xml version="1.0"?>
<tool id="get_binding_residue_from_ligand_list" name="GET binding residue from HETATM ID list and UniProtID list.">
  <description></description>
  
  <inputs>
    <param name="lig" format="txt" type="data" label="HETATOM codes" />
    <param name="prot" format="txt" type="data" label="UniProt IDs" />
    <!-- param name="pattern" type="text" value="" label="Pattern" /-->
  </inputs>

  <outputs>
    <data format="txt" name="output" />
  </outputs>

  <command interpreter="sh">get_binding_residue_from_ligand_list.sh '$lig' '$prot' '$output'</command>

  <help>
  </help>
</tool>
